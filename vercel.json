{
	"version": 2,
	"builds": [
		{
			"src": "api/user.js",
			"use": "@vercel/node"
		},
		{
			"src": "api/top-users.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/user/(.*)",
			"dest": "/api/user.js"
		},
		{
			"src": "/api/top-users",
			"dest": "/api/top-users.js"
		}
	]
}
